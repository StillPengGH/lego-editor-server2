{
  "name": "lego-editor-server",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "node bin/www",
    "dev": "cross-env NODE_ENV=dev ./node_modules/.bin/nodemon bin/www",
    "prd": "pm2 start bin/www",
    "prd-dev": "cross-env NODE_ENV=dev pm2 start bin/pm2-prd-dev.config.js",
    "test:local": "cross-env NODE_ENV=test_local jest --runInBand --passWithNoTests --colors --forceExit",
    "test:remote": "cross-env NODE_ENV=test_remote jest --runInBand --passWithNoTests --colors --forceExit"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "cross-env": "^7.0.3",
    "debug": "^4.1.1",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.7.0",
    "koa-bodyparser": "^4.2.1",
    "koa-convert": "^1.2.0",
    "koa-json": "^2.0.2",
    "koa-jwt": "^4.0.3",
    "koa-logger": "^3.2.0",
    "koa-onerror": "^4.1.0",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "koa-views": "^6.2.0",
    "mongoose": "^6.0.12",
    "mysql2": "^2.3.3-rc.0",
    "pug": "^2.0.3",
    "redis": "^3.1.2",
    "require-all": "^3.0.0",
    "sequelize": "^6.9.0",
    "simple-git": "^2.47.0",
    "supertest": "^6.1.6"
  },
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "jest": "^27.3.1",
    "nodemon": "^1.19.1"
  }
}
